import "TimerInf.camkes";

component Timerbase{
    hardware;
    dataport Buf reg;
    emits DataAvailable irq;
}

component Timer {
    dataport Buf            reg;
    consumes DataAvailable  irq;

    provides timer_inf      Timer;
    has semaphore           sem;
}