import "UartInf.camkes";
import <seos_if_chanMux.camkes>;


component UartDev {
    hardware;
    dataport Buf uartRegBase;
}

component UartDrv {
    control;

    dataport Buf            uartRegBase;
    dataport Buf            inputDataPort;

    provides UartInf        UartDrv;
    uses     ChanMuxOutInf  Output;
}
